u4fbf\u5c06\u635f\u5931\u964d\u4f4e\u5230\u6700\u5c0f\u3002 \u4f7f\u7528\u96c6\u56e2\u8d39\u63a7\u5546\u65c5\u5e73\u53f0\u7684\u5458\u5de5\uff0c\u5728\u51fa\u5dee\u76ee\u7684\u5730\u6216\u8005\u51fa\u5dee\u65e5\u671f\u6709\u53d8\u5316\u65f6\uff0c\u9700\u8981\u5728\u539f\u7533\u8bf7\u5355\u57fa\u7840\u4e0a\u63d0\u4ea4\u51fa\u5dee\u7533\u8bf7\u53d8\u66f4\u5355\u5e76\u5b8c\u6210\u5ba1\u6279\u3002 \u5982\u51fa\u5dee\u8ba1\u5212\u53d6\u6d88\uff0c\u5458\u5de5\u987b\u53ca\u65f6\u529e\u7406\u9000\u7968\u3002 4.  \u4e58\u5750\u706b\u8f66\uff0c\u4ece\u665a\u4e0a 20 \u65f6\u81f3\u6b21\u65e5 7 \u65f6\u4e4b\u95f4\u4e58\u8f66\u65f6\u95f4 6 \u5c0f\u65f6\u4ee5\u4e0a\u6216\u5176\u4ed6\u65f6\u95f4\u8fde\u7eed\u4e58\u8f6610\u5c0f\u65f6\u4ee5\u4e0a\uff0c\u53ef\u8d2d\u4e70\u540c\u5e2d\u5367\u94fa\u8f66\u7968\u3002 5.  \u516c\u53f8\u5df2\u7edf\u4e00\u4e3a\u5458\u5de5\u8d2d\u4e70\u4ea4\u901a\u610f\u5916\u4fdd\u9669\uff0c\u4e0d\u518d\u62a5\u9500\u4e2a\u4eba\u8d2d\u4e70\u7684\u4ea4\u901a\u610f\u5916\u4fdd\u9669\u8d39\u3002 6.  \u4e0d\u540c\u57ce\u5e02\u4e4b\u95f4\u7684\u5f80\u6765\uff0c\u53ea\u80fd\u4e58\u5750\u957f\u9014\u6c7d\u8f66\u6216\u706b\u8f66\u3002 \u4e58\u5750\u51fa\u79df\u8f66\u7684\u6309\u76f8\u5e94\u957f\u9014\u6c7d\u8f66\u6216\u706b\u8f66\u7968\u4ef7\u62a5\u9500\u3002 7.  \u7f51\u7ea6\u8f66\u5e94\u4e58\u5750\u7ecf\u6d4e\u578b\u7f51\u7ea6\u8f66\u3002 8.  \u672a\u4f7f\u7528\u96c6\u56e2\u8d39\u63a7\u5546\u65c5\u5e73\u53f0\u7684\u5458\u5de5\u7533\u8bf7\u673a\u7968\u62a5\u9500\u5fc5\u987b\u63d0\u4f9b\u884c\u7a0b\u5355\u3002 \u4ee5\u8ba2\u7968\u673a\u6784\u5f00\u5177\u7684\u53d1\u7968\u8fdb\u884c\u62a5\u9500\u6216\u673a\u7968\u6539\u7b7e\u65e0\u5b9e\u9645\u4e58\u5750\u65e5\u671f\u7684\uff0c\u5fc5\u987b\u63d0\u4f9b\u767b\u673a\u724c\u3002 \u7b2c506\u6761 \u76ee\u7684\u5730\u5e02\u5185\u4ea4\u901a\u8d39\n[2]\u62a5\u9500\u4e0d\u7528\u63d0\u4ea4\u53d1\u7968\u3002 \u51fa\u5dee\u4eba\u5458\u5df2\u62a5\u9500\u5dee\u65c5\u8865\u8d34\u7684\uff0c\u4e0d\u518d\u9886\u53d6\u5348\u9910\u8865\u8d34\u3002 \u5883\u5185\u5dee\u65c5\u8865\u8d34\u6807\u51c6\u4e3a60\u5143/\u4eba\u5929\uff0c\u897f\u85cf\u548c\u9752\u6d772\u4e2a\u7279\u6b8a\u5730\u533a\u5dee\u65c5\u8865\u8d34\u6309\u7167120\u5143/\u4eba\u5929\u8ba1\u7b97\u3002 \u5230\u5f02\u5730\u53c2\u52a0\u5b89\u6392\u98df\u5bbf\u7684\u4f1a\u8bae\u3001\u57f9\u8bad\u73ed\u7b49\u65e0\u5dee\u65c5\u8865\u8d34\uff1b \u6839\u636e\u4f1a\u8bae\u6587\u4ef6\u6216\u901a\u77e5\u89c4\u5b9a\u4e0d\u5b89\u6392\u98df\u5bbf\u7684\uff0c\u89c6\u540c\u51fa\u5dee\u8ba1\u7b97\u8865\u8d34\u3002 \u5883\u5916\u5dee\u65c5\u8865\u8d34\u6309\u7167\u201c\u9644\u4ef62\uff1a\u5dee\u65c5\u6807\u51c6\u989d\u5ea6\u5217\u8868\u201d\u4e2d\u7684\u6807\u51c6\u6267\u884c\u3002 \u672a\u4f7f\u7528\u65b0\u8d39\u63a7\u7cfb\u7edf\u7684\uff0c\u5458\u5de5\u62a5\u9500\u8865\u8d34\u65f6\u5fc5\u987b\u63d0\u4f9b\u5f80\u8fd4\u5f02\u5730\u7684\u884c\u7a0b\u4f9d\u636e\uff08\u5982\uff1a\u673a\u7968\u3001\u706b\u8f66\u7968\u3001\u957f\u9014\u8f66\u7968\u6216\u4f4f\u5bbf\u7ed3\u7b97\u5355\u7b49\uff09\uff0c\u5982\u679c\u6ca1\u6709\u539f\u59cb\u5355\u636e\u7684\uff0c\u53ef\u63d0\u4f9b\u590d\u5370\u4ef6\u3002 \u8d85\u8fc7\u4e24\u4e2a\u6708\u7684\u957f\u671f\u51fa\u5dee\u6216\u65e0\u884c\u7a0b\u4f9d\u636e\u65f6\u9700\u63d0\u4f9b\u5b9e\u5730\u6253\u5361\u7684\u8003\u52e4\u8bb0\u5f55\u3002 \u7b2c510\u6761 \u5176\u4ed6\u4e8b\u9879 \u8d81\u51fa\u5dee\u5de5\u4f5c\u4e4b\u4fbf\uff0c\u4e8b\u5148\u7ecf\u4e00\u7ea7\u90e8\u95e8\u603b\u7ecf\u7406\u6279\u51c6\u5c31\u8fd1\u56de\u5bb6\u63a2\u4eb2\u6216\u529e\u4e8b\uff0c\u5176\u7ed5\u9053\u4ea4\u901a\u8d39\u7528\u8d85\u8fc7\u51fa\u5dee\u76f4\u7ebf\u5355\u7a0b\u4ea4\u901a\u8d39\u7968\u4ef7\u591a\u5f00\u652f\u90e8\u5206\u7531\u4e2a\u4eba\u81ea\u7406\uff0c\u516c\u53f8\u4e0d\u627f\u62c5\u7ed5\u9053\u548c\u5728\u5bb6\u671f\u95f4\u7684\u5dee\u65c5\u8865\u8d34\u548c\u4f4f\u5bbf\u8d39\u3001\u4ea4\u901a\u8d39\u3002 \u5dee\u65c5\u8d39\u62a5\u9500\u539f\u5219\u4e0a\u5e94\u6309\u4e00\u4e2a\u5b8c\u6574\u7684\u51fa\u5dee\u5468\u671f\u62a5\u9500\uff0c\u5bf9\u4e8e\u8fde\u7eed\u4e24\u4e2a\u6708\u4ee5\u4e0a\u7684\u957f\u671f\u51fa\u5dee\u6bcf2\u4e2a\u6708\u5e94\u63d0\u4ea4\u4e00\u6b21\u3002 \u957f\u9014\u51fa\u5dee\u4e0d\u63a8\u8350\u91c7\u7528\u81ea\u9a7e\u8f66\u51fa\u5dee\u65b9\u5f0f\u3002 \u5728\u516c\u7528\u4ea4\u901a\u4e0d\u4fbf\u5229\u60c5\u51b5\u4e0b\uff0c\u9700\u4e8b\u5148\u7ecf\u8fc7\u4e00\u7ea7\u90e8\u95e8\u8d1f\u8d23\u4eba\u6279\u51c6\uff0c\u53ef\u51ed\u7968\u62a5\u9500\u81ea\u9a7e\u51fa\u5dee\uff08\u5355\u7a0b4\u5c0f\u65f6\u4ee5\u5185\uff09\u7684\u6cb9\u8d39\u3001\u8def\u6865\u8d39\u7528\u548c\u505c\u8f66\u8d39\u7b49\u3002 \u5728\u62a5\u9500\u65f6\u9700\u4ee5\u9644\u4ef6\u63d0\u4ea4\u4e8b\u5148\u5ba1\u6279\u90ae\u4ef6\uff0c\u62a5\u9500\u91d1\u989d\u4ee5 \u516c\u91ccx0.\n[3]\u7b2c\u516b\u7ae0 \u9644\u5219 \u7b2c801\u6761 \u672c\u5236\u5ea6\u81ea\u7b7e\u53d1\u4e4b\u65e5\u8d77\u6267\u884c\uff0c\u4e4b\u524d\u53d1\u5e03\u7684\u4e0e\u6b64\u76f8\u5173\u5236\u5ea6\u4e00\u5e76\u5e9f\u6b62\u3002 \u7b2c802\u6761 \u672c\u5236\u5ea6\u7684\u89e3\u91ca\u6743\u5f52\u96c6\u56e2\u8d22\u52a1\u7ba1\u7406\u90e8\u3002 \u9644\u4ef61\uff1a\u5dee\u65c5\u4ea4\u901a\u5de5\u5177\u6807\u51c6 \u9644\u4ef62\uff1a\u5dee\u65c5\u6807\u51c6\u5217\u8868 \u9644\u4ef63\uff1a\u4ea4\u901a\u8d39\u6807\u51c6\u5217\u8868 \u9644\u4ef61 \u5dee\u65c5\u4ea4\u901a\u5de5\u5177\u6807\u51c6 <table>[{\"\u62a5\u9500\u6807\u51c6\u5206\u7c7b\": \"\u4e00\u7c7b\",  \"\u804c\u7ea7\u5bf9\u5e94\": \"\u96c6\u56e2\u8463\u4e8b\u4f1a\u6210\u5458\",  \"\u957f\u9014\u4ea4\u901a\u8d39 \u98de\u673a\": \"\u5b9e\u62a5\u5b9e\u9500\",  \"\u957f\u9014\u4ea4\u901a\u8d39 \u706b\u8f66\": \"\u5b9e\u62a5\u5b9e\u9500\",  \"\u957f\u9014\u4ea4\u901a\u8d39 \u8f6e\u8239\": \"\u5b9e\u62a5\u5b9e\u9500\",  \"\u957f\u9014\u4ea4\u901a\u8d39 \u957f\u9014\u6c7d\u8f66\": \"\u5b9e\u62a5\u5b9e\u9500\",  \"\u957f\u9014\u4ea4\u901a\u8d39 \u5f80\u8fd4\u573a\u7ad9\u7801\u5934\u4ea4\u901a\u8d39\": \"\u5b9e\u62a5\u5b9e\u9500\"},  {\"\u62a5\u9500\u6807\u51c6\u5206\u7c7b\": \"\u4e8c\u7c7b\",  \"\u804c\u7e